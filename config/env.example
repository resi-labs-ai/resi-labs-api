# Example Environment Configuration
# Copy this to .env.development or .env.production and update values

# Database
DATABASE_URL=postgresql+asyncpg://localhost:5432/zipcode_dev
SQL_DEBUG=false
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# Existing S3 Configuration
S3_BUCKET=1000-resilabs-caleb-dev-bittensor-sn46-datacollection
S3_REGION=us-east-2
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here

# New S3 Configuration for Validators
S3_VALIDATOR_BUCKET=resi-validated-data-dev
VALIDATOR_S3_SESSION_DURATION=14400

# Bittensor Configuration
BT_NETWORK=test
NET_UID=46

# Zipcode System Configuration
TARGET_LISTINGS=5000
TOLERANCE_PERCENT=15
EPOCH_DURATION_HOURS=4
COOLDOWN_HOURS=24
MIN_ZIPCODE_LISTINGS=200
MAX_ZIPCODE_LISTINGS=3000

# State Priorities (configurable - lower number = higher priority)
STATE_PRIORITIES=PA:1,NJ:2,NY:3,DE:4,MD:5

# Security Configuration
ZIPCODE_SECRET_KEY=dev-secret-key-change-in-production-32chars
VALIDATOR_MIN_STAKE=100  # Lower for testing

# Algorithm Parameters (adjustable for gaming mitigation)
PREMIUM_WEIGHT=1.5
STANDARD_WEIGHT=1.0
EMERGING_WEIGHT=0.8
SELECTION_RANDOMNESS=0.25
GEOGRAPHIC_SPREAD_FACTOR=0.15
HONEYPOT_PROBABILITY=0.3
HONEYPOT_THRESHOLD=50

# Rate Limiting
DAILY_LIMIT_PER_MINER=20
DAILY_LIMIT_PER_VALIDATOR=10000
TOTAL_DAILY_LIMIT=200000

# Server Configuration
PORT=8000
LOG_LEVEL=INFO

# Redis (optional for development)
# REDIS_URL=redis://localhost:6379
